{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "godot-quickjs",
      "request": "launch",
      "name": "Launch Godot",
      "sourceMaps": true,
      "program": "/Applications/Godot ECMAScript.app/Contents/MacOS/Godot"
    },
    {
      "type": "godot-quickjs",
      "request": "attach",
      "name": "Attach to Godot",
      "hostname": "localhost",
      "port": 5556
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Build Script",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/build.js",
      "cwd": "${workspaceFolder}/scripts",
      "outFiles": ["${workspaceFolder}/scripts/**/*.js"]
    }
  ]
}
